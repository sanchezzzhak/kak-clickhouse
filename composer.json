{
    "name": "kak/clickhouse",
    "description": "ClickHouse for Yii2",
    "type": "yii2-extension",
    "keywords": ["yii2","ClickHouse","ClickHouse ActiveRecord Yii2"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/sanchezzzhak/kak-clickhouse/issues?state=open",
        "source": "https://github.com/sanchezzzhak/kak-clickhouse"
    },
    "authors": [
        {
            "name": "Tutik Alexsandr",
            "email": "sanchezzzhak@ya.ru"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "yiisoft/yii2-httpclient": "^2.0.14"
    },
    "require-dev": {
      "symfony/event-dispatcher-contracts": "2.2.0",
      "codeception/codeception": "4.1.22",
      "codeception/module-asserts": "1.3",
      "codeception/module-yii2": "1.1"
    },
    "autoload": {
        "psr-4": {
            "kak\\clickhouse\\": ""
        }
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
